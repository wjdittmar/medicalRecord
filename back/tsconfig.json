{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "../dist/back",
    "paths": {
      "@models/*": ["src/models/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
